    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "http-server": "^0.9.0",
    "install": "^0.8.9",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "npm": "^4.5.0",
    "npm-run-all": "^3.0.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.15.0"
